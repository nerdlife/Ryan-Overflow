       <%= form_for @user do |f| %>
       	<%= f.error_messages %>
  		<div class="form-group">
			<div class="ryan-overflow-form-label">
				Email
			</div>
			<div class="col-md-8 col-md-offset-2">
				<%= f.text_field :email, :placeholder => 'Email Address', class: "form-control" %>
			</div>
		</div>
		<br>
		<br>

		<div class="form-group">
			<div class="ryan-overflow-form-label">
				Name
			</div>
			<div class="col-md-8 col-md-offset-2">
				<%= f.text_field :name, :placeholder => 'Your Name', class: "form-control" %>
			</div>
		</div>
		<br>
		<br>

		<div class="form-group">
			<div class="ryan-overflow-form-label">
				Username
			</div>
			<div class="col-md-8 col-md-offset-2">
				<%= f.text_field :username, placeholder: 'Username', class: "form-control input-xlg" %>
			</div>
		</div>
		<br>
		<br>

		<div class="form-group">
			<div class="ryan-overflow-form-label">
				Password
			</div>
			<div class="col-md-8 col-md-offset-2">
				<%= f.text_field :password, placeholder: 'Password', class: "form-control input-xlg" %>
			</div>
		</div>
		<br>
		<br>

		<div class="form-group">
			<div class="ryan-overflow-form-label">
				Confirm Password
			</div>
			<div class="col-md-8 col-md-offset-2">
				<%= f.text_field :password_confirmation, placeholder: 'Password', class: "form-control input-xlg" %>
			</div>
		</div>
		<br>
		<br>

		<div class="form-group">
			<div class="ryan-overflow-form-label">
				Profile Bio
			</div>
			<div class="col-md-8 col-md-offset-2">
				<%= f.text_field :bio, placeholder: 'Bio...', class: "form-control input-xlg" %>
			</div>
		</div>
		<br>
		<br>

		<div class="form-group">
			<div class="ryan-overflow-form-label">
				Profile Pic
			</div>
			<div class="col-md-8 col-md-offset-2">
				<%= f.text_field :profile_pic, placeholder: 'Url of Pic', class: "form-control input-xlg" %>
			</div>
		</div>
		<br>
		<br>
		
				<div class="ryan-overflow-form-button">
					<%= f.submit "Submit", class: "btn btn-default btn-primary" %>
				</div>
			

	<% end %>
		
